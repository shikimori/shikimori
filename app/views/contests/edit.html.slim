      a.back[
        href="#{contest_url @resource}"
        title=t('.back_to_contest_page')
        data-text="#{t 'back'}"
      ]
    = @page_title.last

  = render partial: 'blocks/breadcrumbs', object: @breadcrumbs

= render 'form', contest: @resource
